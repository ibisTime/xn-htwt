<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.cdkj.loan.dao.IEnterFileListDAO">    <resultMap id="enterFileList_DB_Result" type="enterFileList">        <result column="code" property="code"/>        <result column="biz_code" property="bizCode"/>        <result column="content" property="content"/>        <result column="file_count" property="fileCount"/>        <result column="deposit_date_time" property="depositDateTime"/>        <result column="operator" property="operator"/>        <result column="remark" property="remark"/>        <result column="operator_name" property="operatorName"/>    </resultMap>    <sql id="where_condition">        <trim prefix="WHERE" prefixOverrides="AND | OR">            <if test="code != null and code != '' ">                AND t.code = #{code}            </if>            <if test="bizCode != null and bizCode != '' ">                AND t.biz_code = #{bizCode}            </if>            <if test="operator != null and operator != '' ">                AND t.operator = #{operator}            </if>        </trim>    </sql>    <select id="select_enterFileList" parameterType="enterFileList"            resultMap="enterFileList_DB_Result">        SELECT        t.code        ,t.biz_code        ,t.content        ,t.file_count        ,t.deposit_date_time        ,t.operator        ,t.remark        ,u.real_name as operator_name        FROM        tdq_enter_file_list t        left join tsys_user u on u.user_id = t.operator        <include refid="where_condition"/>        <trim prefix="ORDER BY ">            <if test="order != null and order != '' ">                t.${order}            </if>        </trim>    </select>    <select id="select_enterFileList_count" parameterType="enterFileList"            resultType="java.lang.Long">        SELECT count(1) FROM tdq_enter_file_list t        <include refid="where_condition"/>    </select>    <insert id="insert_enterFileList" parameterType="enterFileList">		INSERT INTO		tdq_enter_file_list(		code		,biz_code		,content		,file_count		,deposit_date_time		,operator		,remark		)		VALUES(		#{code}		,#{bizCode}		,#{content}		,#{fileCount}		,#{depositDateTime}		,#{operator}		,#{remark}		)	</insert>    <update id="update_enterFileList" parameterType="enterFileList">		UPDATE tdq_enter_file_list		SET		biz_code = #{bizCode}		,content = #{content}		,file_count = #{fileCount}		,deposit_date_time = #{depositDateTime}		,operator = #{operator}		,remark = #{remark}		WHERE code = #{code}	</update>    <delete id="delete_enterFileList" parameterType="enterFileList">		delete from tdq_enter_file_list		WHERE code = #{code}	</delete></mapper>